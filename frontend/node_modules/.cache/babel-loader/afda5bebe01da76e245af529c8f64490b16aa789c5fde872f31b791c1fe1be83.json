{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport { ref, onMounted, onBeforeUnmount, nextTick } from 'vue';\nimport axios from 'axios';\nimport Swal from 'sweetalert2';\nimport 'select2';\nimport feather from 'feather-icons';\nimport ClipLoader from 'vue-spinner/src/ClipLoader.vue';\nimport $ from 'jquery';\nimport Navbar from '/src/components/Admin/Navbar.vue';\nexport default {\n  components: {\n    Navbar,\n    ClipLoader\n  },\n  setup() {\n    const apiURL = process.env.VUE_APP_URL;\n    const imageURL = process.env.VUE_APP_IMAGE_URL;\n    const brands = ref([]);\n    const newBrand = ref({\n      name: '',\n      logo: {\n        file: null,\n        url: null\n      },\n      status: 'active'\n    });\n    const editBrand = ref({\n      _id: '',\n      name: '',\n      logo: {\n        file: null,\n        url: null\n      },\n      status: 'active',\n      isLogoRemoved: false\n    });\n    const loading = ref(false);\n    const getBrands = async () => {\n      loading.value = true;\n      try {\n        const response = await axios.get(`${apiURL}/brands_list`);\n        if (response.data.success) {\n          // Update the brands array with the new data\n          brands.value = response.data.brandList;\n\n          // Reinitialize DataTable if necessary\n          nextTick(() => {\n            const table = $('.table');\n            if ($.fn.DataTable.isDataTable(table)) {\n              table.DataTable().destroy(); // Destroy the existing instance\n            }\n            table.DataTable(); // Reinitialize\n          });\n        } else {\n          Swal.fire('Error', response.data.message, 'error');\n        }\n      } catch (error) {\n        Swal.fire('Error', 'Failed to get brands', 'error');\n      } finally {\n        loading.value = false;\n      }\n    };\n    const addBrand = async () => {\n      try {\n        const formData = new FormData();\n        formData.append('name', newBrand.value.name);\n        formData.append('status', newBrand.value.status);\n        if (newBrand.value.logo.file) {\n          formData.append('logo', newBrand.value.logo.file);\n        }\n        const response = await axios.post(`${apiURL}/add_brand`, formData);\n        if (response.data.success) {\n          Swal.fire('Success', 'Brand successfully added!', 'success').then(() => {\n            const cancelBtn = document.querySelector('#add-brand .btn-cancel');\n            if (cancelBtn) {\n              cancelBtn.click();\n              nextTick(() => {\n                getBrands();\n              });\n            } else {\n              console.warn('#add-brand .btn-cancel not found');\n              getBrands();\n            }\n          });\n        } else {\n          Swal.fire('Error', response.data.message, 'error');\n        }\n      } catch (error) {\n        Swal.fire('Error', 'Failed to add brand', 'error');\n      }\n    };\n    const openEditModal = async id => {\n      const response = await axios.get(`${apiURL}/get_brand/${id}`);\n      const brand = response.data.brand;\n      editBrand.value = {\n        _id: brand._id,\n        name: brand.name,\n        logo: {\n          url: `${imageURL}${brand.logo}`,\n          file: null\n        },\n        status: brand.status\n      };\n    };\n    const updateBrand = async () => {\n      try {\n        const formData = new FormData();\n        formData.append('name', editBrand.value.name); // Ensure `editBrand.value.name` exists\n        formData.append('status', editBrand.value.status);\n        formData.append('logoRemoved', editBrand.value.isLogoRemoved); // Ensure `editBrand.value.status` exists\n        if (editBrand.value.logo.file) {\n          formData.append('logo', editBrand.value.logo.file); // Only append if a new file is selected\n        }\n        const response = await axios.patch(`${apiURL}/edit_brand/${editBrand.value._id}`, formData);\n        if (response.data.success) {\n          Swal.fire('Success', 'Brand updated successfully!', 'success').then(() => {\n            document.querySelector('#edit-brand .btn-cancel').click(); // Close modal\n          });\n        } else {\n          Swal.fire('Error', response.data.message, 'error');\n        }\n        getBrands(); // Refresh the brand list\n      } catch (error) {\n        console.error('Error in updateBrand:', error);\n        Swal.fire('Error', error.message, 'error');\n      }\n    };\n    const deleteBrand = async id => {\n      Swal.fire({\n        title: 'Are you sure?',\n        text: 'You are about to delete this brand!',\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonText: 'Yes, delete it!'\n      }).then(async result => {\n        if (result.isConfirmed) {\n          try {\n            const response = await axios.patch(`${apiURL}/edit_brand/${id}`, {\n              status: 'deleted',\n              deletedAt: Date.now()\n            });\n            if (response.data.success) {\n              Swal.fire('Success', 'Brand deleted successfully!', 'success');\n              getBrands();\n            } else {\n              Swal.fire('Error', response.data.message, 'error');\n            }\n          } catch (error) {\n            Swal.fire('Error', 'Failed to delete brand', 'error');\n          }\n        }\n      });\n    };\n    const triggerFileInput = brand => {\n      if (brand === 'new') {\n        document.querySelector('#new-brand-file-input').click();\n      } else if (brand === 'edit') {\n        document.querySelector('#edit-brand-file-input').click();\n      }\n    };\n    const onFileChange = (brand, event) => {\n      const file = event.target.files[0];\n      if (file) {\n        const logoData = {\n          file,\n          url: URL.createObjectURL(file)\n        };\n        if (brand === 'new') {\n          newBrand.value.logo = logoData;\n        } else if (brand === 'edit') {\n          editBrand.value.logo = logoData;\n        }\n      }\n    };\n    const removeImage = brand => {\n      if (brand === 'new') {\n        newBrand.value.logo = {\n          file: null,\n          url: null\n        };\n      } else if (brand === 'edit') {\n        editBrand.value.logo = {\n          file: null,\n          url: null\n        };\n        editBrand.value.isLogoRemoved = true;\n      }\n      console.log(editBrand.value.logo);\n    };\n    onMounted(async () => {\n      try {\n        await nextTick();\n        await getBrands();\n        // nextTick(async () =>{\n        //     const table = document.querySelector('.table');\n        //     if (table) {\n        //         $(table).DataTable();\n        //     }\n        // })\n\n        await feather.replace();\n      } catch (error) {\n        console.error(error);\n      }\n    });\n    return {\n      brands,\n      newBrand,\n      editBrand,\n      loading,\n      getBrands,\n      addBrand,\n      updateBrand,\n      deleteBrand,\n      openEditModal,\n      triggerFileInput,\n      onFileChange,\n      removeImage,\n      imageURL,\n      apiURL\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onBeforeUnmount","nextTick","axios","Swal","feather","ClipLoader","$","Navbar","components","setup","apiURL","process","env","VUE_APP_URL","imageURL","VUE_APP_IMAGE_URL","brands","newBrand","name","logo","file","url","status","editBrand","_id","isLogoRemoved","loading","getBrands","value","response","get","data","success","brandList","table","fn","DataTable","isDataTable","destroy","fire","message","error","addBrand","formData","FormData","append","post","then","cancelBtn","document","querySelector","click","console","warn","openEditModal","id","brand","updateBrand","patch","deleteBrand","title","text","icon","showCancelButton","confirmButtonText","result","isConfirmed","deletedAt","Date","now","triggerFileInput","onFileChange","event","target","files","logoData","URL","createObjectURL","removeImage","log","replace"],"sources":["C:\\Users\\Administrator\\OneDrive\\Desktop\\POSExpress\\frontend\\src\\components\\Admin\\Inventory\\Brands.vue"],"sourcesContent":["<template>\r\n    <Navbar />\r\n\r\n    <div class=\"page-wrapper\">\r\n        <ClipLoader v-if=\"loading\" />\r\n        <div class=\"content\">\r\n            <div class=\"page-header\">\r\n                <div class=\"add-item d-flex\">\r\n                    <div class=\"page-title\">\r\n                        <h4>Brand</h4>\r\n                        <h6>Manage your brands</h6>\r\n                    </div>\r\n                </div>\r\n                <ul class=\"table-top-head\">\r\n                    <li>\r\n                        <a data-bs-toggle=\"tooltip\" data-bs-placement=\"top\" title=\"Pdf\"><img\r\n                                src=\"/assets/img/icons/pdf.svg\" alt=\"img\" /></a>\r\n                    </li>\r\n                    <li>\r\n                        <a data-bs-toggle=\"tooltip\" data-bs-placement=\"top\" title=\"Excel\"><img\r\n                                src=\"/assets/img/icons/excel.svg\" alt=\"img\" /></a>\r\n                    </li>\r\n                    <li>\r\n                        <a data-bs-toggle=\"tooltip\" data-bs-placement=\"top\" title=\"Print\"><i data-feather=\"printer\"\r\n                                class=\"feather-rotate-ccw\"></i></a>\r\n                    </li>\r\n                    <li>\r\n                        <a data-bs-toggle=\"tooltip\" data-bs-placement=\"top\" title=\"Refresh\"><i data-feather=\"rotate-ccw\"\r\n                                class=\"feather-rotate-ccw\"></i></a>\r\n                    </li>\r\n                    <li>\r\n                        <a data-bs-toggle=\"tooltip\" data-bs-placement=\"top\" title=\"Collapse\" id=\"collapse-header\"><i\r\n                                data-feather=\"chevron-up\" class=\"feather-chevron-up\"></i></a>\r\n                    </li>\r\n                </ul>\r\n                <div class=\"page-btn\">\r\n                    <a href=\"#\" class=\"btn btn-added\" data-bs-toggle=\"modal\" data-bs-target=\"#add-brand\"><i\r\n                            data-feather=\"plus-circle\" class=\"me-2\"></i>Add New Brand</a>\r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"card table-list-card\">\r\n                <div class=\"card-body\">\r\n                    <div class=\"table-responsive\">\r\n\r\n                        <table class=\"table datanew\" id=\"brand-table\">\r\n                            <thead>\r\n                                <tr>\r\n                                    <th>Brand</th>\r\n                                    <th>Logo</th>\r\n                                    <th>Status</th>\r\n                                    <th>Created On</th>\r\n                                    <th>Last Updated</th>\r\n                                    <th class=\"no-sort\">Action</th>\r\n                                </tr>\r\n                            </thead>\r\n\r\n                            <tbody>\r\n\r\n                                <tr v-for=\"brand in brands\" :key=\"brand.id\">\r\n                                    <td>{{ brand.name }}</td>\r\n                                    <td>\r\n                                        <span class=\"d-flex\">\r\n                                            <img v-if=\"brand.logo\" :src=\"`${imageURL}${brand.logo}`\"\r\n                                                class=\"img-thumbnail\"\r\n                                                style=\"width: 150px; height: 50px; object-fit: contain;\" />\r\n                                            <img v-else :src=\"`/img/icons/no-image-icon.png`\"\r\n                                                alt=\"No image available\" />\r\n                                        </span>\r\n                                    </td>\r\n                                    <td>\r\n                                        <span :class=\"{\r\n                                            'badge': true,\r\n                                            'badge-linesuccess': brand.status === 'active',\r\n                                            'badge-linewarning': brand.status === 'inactive'\r\n                                        }\">\r\n                                            {{ brand.status }}\r\n                                        </span>\r\n                                    </td>\r\n                                    <td>{{ $formatDate(brand.createdAt) }}</td>\r\n                                    <td>{{ $formatDate(brand.updatedAt) }}</td>\r\n                                    <td class=\"action-table-data\">\r\n                                        <div class=\"edit-delete-action\">\r\n                                            <a class=\"me-2 p-2\" href=\"#\" data-bs-toggle=\"modal\"\r\n                                                data-bs-target=\"#edit-brand\" @click=\"openEditModal(brand._id)\">\r\n                                                <i data-feather=\"edit\" class=\"feather-edit\"></i>\r\n                                            </a>\r\n                                            <a class=\"confirm-text p-2\" href=\"javascript:void(0);\"\r\n                                                @click=\"deleteBrand(brand._id)\">\r\n                                                <i data-feather=\"trash-2\" class=\"feather-trash-2\"></i>\r\n                                            </a>\r\n                                        </div>\r\n                                    </td>\r\n                                </tr>\r\n\r\n                            </tbody>\r\n\r\n\r\n                        </table>\r\n\r\n\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n\r\n    <div class=\"modal fade\" id=\"add-brand\">\r\n        <div class=\"modal-dialog modal-dialog-centered custom-modal-two\">\r\n            <div class=\"modal-content\">\r\n                <div class=\"page-wrapper-new p-0\">\r\n                    <div class=\"content\">\r\n                        <div class=\"modal-header border-0 custom-modal-header\">\r\n                            <div class=\"page-title\">\r\n                                <h4>Create Brand</h4>\r\n                            </div>\r\n                            <button type=\"button\" class=\"close\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                <span aria-hidden=\"true\">&times;</span>\r\n                            </button>\r\n                        </div>\r\n                        <div class=\"modal-body custom-modal-body new-employee-field\">\r\n                            <form @submit.prevent=\"addBrand\">\r\n                                <div class=\"mb-3\">\r\n                                    <label class=\"form-label\">Brand</label>\r\n                                    <input type=\"text\" v-model=\"newBrand.name\" class=\"form-control\" />\r\n                                </div>\r\n                                <label class=\"form-label\">Logo</label>\r\n                                <div class=\"profile-pic-upload mb-3\">\r\n                                    <!-- Add Image -->\r\n                                    <div v-if=\"!newBrand.logo.url\" class=\"profile-pic brand-pic\"\r\n                                        @click=\"triggerFileInput('new')\">\r\n                                        <input id=\"new-brand-file-input\" type=\"file\"\r\n                                            @change=\"onFileChange('new', $event)\" hidden />\r\n                                        <span><i data-feather=\"plus-circle\" class=\"plus-down-add\"></i> Add Image</span>\r\n                                    </div>\r\n\r\n                                    <!-- Image Preview -->\r\n                                    <div class=\"phone-img\" v-else>\r\n                                        <div class=\"image-preview-wrapper\">\r\n                                            <img :src=\"newBrand.logo.url\" alt=\"Uploaded image\" class=\"image-preview\" />\r\n                                            <a @click=\"removeImage('new')\" class=\"remove-image-btn\">\r\n                                                <font-awesome-icon icon=\"xmark\" style=\"color: #ffffff;\" class=\"mx-2\" />\r\n                                            </a>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n\r\n                        \r\n                        <div class=\"mb-0\">\r\n                            <div class=\"status-toggle modal-status d-flex justify-content-between align-items-center\">\r\n\r\n                                <span class=\"form-label\">{{ newBrand.status }}</span>\r\n                                <input type=\"checkbox\" id=\"add-category-status\" class=\"check\" v-model=\"newBrand.status\"\r\n                                    :true-value=\"'active'\" :false-value=\"'inactive'\" />\r\n                                <label for=\"add-category-status\" class=\"checktoggle\"></label>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"modal-footer-btn\">\r\n                            <button type=\"button\" class=\"btn btn-cancel me-2\" data-bs-dismiss=\"modal\">\r\n                                Cancel\r\n                            </button>\r\n                            <button type=\"submit\" class=\"btn btn-submit\">\r\n                                Create Brand\r\n                            </button>\r\n                        </div>\r\n                        </form>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    </div>\r\n\r\n    <div class=\"modal fade\" id=\"edit-brand\">\r\n        <div class=\"modal-dialog modal-dialog-centered custom-modal-two\">\r\n            <div class=\"modal-content\">\r\n                <div class=\"page-wrapper-new p-0\">\r\n                    <div class=\"content\">\r\n                        <div class=\"modal-header border-0 custom-modal-header\">\r\n                            <div class=\"page-title\">\r\n                                <h4>Edit Brand</h4>\r\n                            </div>\r\n                            <button type=\"button\" class=\"close\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                                <span aria-hidden=\"true\">&times;</span>\r\n                            </button>\r\n                        </div>\r\n                        <div class=\"modal-body custom-modal-body new-employee-field\">\r\n                            <form @submit.prevent=\"updateBrand\">\r\n                                <div class=\"mb-3\">\r\n                                    <label class=\"form-label\">Brand</label>\r\n                                    <input type=\"text\" v-model=\"editBrand.name\" class=\"form-control\" />\r\n                                </div>\r\n                                <label class=\"form-label\">Logo</label>\r\n                                <div class=\"profile-pic-upload mb-3\">\r\n                                    <!-- Add Image -->\r\n                                    <div v-if=\"!editBrand.logo.url\" class=\"profile-pic brand-pic\"\r\n                                        @click=\"triggerFileInput('edit')\">\r\n                                        <input id=\"edit-brand-file-input\" type=\"file\" @change=\"onFileChange('edit', $event)\" hidden />\r\n                                        <span><i data-feather=\"plus-circle\" class=\"plus-down-add\"></i> Add Image</span>\r\n                                    </div>\r\n\r\n                                    <!-- Image Preview -->\r\n                                    <div class=\"phone-img\" v-else>\r\n                                        <div class=\"image-preview-wrapper\">\r\n                                            <img :src=\"editBrand.logo.url\" alt=\"Uploaded image\" class=\"image-preview\" />\r\n                                            <a @click=\"removeImage('edit')\" class=\"remove-image-btn\">\r\n                                                <font-awesome-icon icon=\"xmark\" style=\"color: #ffffff;\" class=\"mx-2\" />\r\n                                            </a>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n\r\n                                <div class=\"mb-0\">\r\n                                    <div\r\n                                        class=\"status-toggle modal-status d-flex justify-content-between align-items-center\">\r\n                                        <span class=\"form-label\">{{ editBrand.status }}</span>\r\n                                        <input type=\"checkbox\" id=\"add-category-status\" class=\"check\"\r\n                                            v-model=\"editBrand.status\" :true-value=\"'active'\"\r\n                                            :false-value=\"'inactive'\" />\r\n                                        <label for=\"add-category-status\" class=\"checktoggle\"></label>\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"modal-footer-btn\">\r\n                                    <button type=\"button\" class=\"btn btn-cancel me-2\" data-bs-dismiss=\"modal\">\r\n                                        Cancel\r\n                                    </button>\r\n                                    <button type=\"submit\" class=\"btn btn-submit\">\r\n                                        Save Changes\r\n                                    </button>\r\n                                </div>\r\n                            </form>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n</template>\r\n<script>\r\n\r\nimport { ref, onMounted, onBeforeUnmount, nextTick } from 'vue';\r\nimport axios from 'axios';\r\nimport Swal from 'sweetalert2';\r\nimport 'select2';\r\nimport feather from 'feather-icons';\r\nimport ClipLoader from 'vue-spinner/src/ClipLoader.vue';\r\nimport $ from 'jquery';\r\nimport Navbar from '/src/components/Admin/Navbar.vue';\r\n\r\n\r\nexport default {\r\n    components: {\r\n        Navbar,\r\n        ClipLoader,\r\n    },\r\n    setup() {\r\n        const apiURL = process.env.VUE_APP_URL;\r\n        const imageURL = process.env.VUE_APP_IMAGE_URL;\r\n        const brands = ref([]);\r\n        const newBrand = ref({\r\n            name: '',\r\n            logo: { file: null, url: null },\r\n            status: 'active',\r\n        });\r\n        const editBrand = ref({\r\n            _id: '',\r\n            name: '',\r\n            logo: { file: null, url: null },\r\n            status: 'active',\r\n            isLogoRemoved: false,\r\n        });\r\n        const loading = ref(false);\r\n\r\n        const getBrands = async () => {\r\n    loading.value = true;\r\n    try {\r\n        const response = await axios.get(`${apiURL}/brands_list`);\r\n        if (response.data.success) {\r\n            // Update the brands array with the new data\r\n            brands.value = response.data.brandList;\r\n\r\n            // Reinitialize DataTable if necessary\r\n            nextTick(() => {\r\n                const table = $('.table');\r\n                if ($.fn.DataTable.isDataTable(table)) {\r\n                    table.DataTable().destroy(); // Destroy the existing instance\r\n                }\r\n                table.DataTable(); // Reinitialize\r\n            });\r\n        } else {\r\n            Swal.fire('Error', response.data.message, 'error');\r\n        }\r\n    } catch (error) {\r\n        Swal.fire('Error', 'Failed to get brands', 'error');\r\n    } finally {\r\n        loading.value = false;\r\n    }\r\n};\r\n\r\n\r\n        const addBrand = async () => {\r\n            try {\r\n                const formData = new FormData();\r\n                formData.append('name', newBrand.value.name);\r\n                formData.append('status', newBrand.value.status);\r\n                if (newBrand.value.logo.file) {\r\n                    formData.append('logo', newBrand.value.logo.file);\r\n                }\r\n\r\n                const response = await axios.post(`${apiURL}/add_brand`, formData);\r\n\r\n                if (response.data.success) {\r\n             \r\n                    Swal.fire('Success', 'Brand successfully added!', 'success').then(() => {\r\n    const cancelBtn = document.querySelector('#add-brand .btn-cancel');\r\n    if (cancelBtn) {\r\n        cancelBtn.click();\r\n        nextTick(() => {\r\n            getBrands();\r\n        });\r\n    } else {\r\n        console.warn('#add-brand .btn-cancel not found');\r\n        getBrands();\r\n    }\r\n});\r\n\r\n                } else {\r\n                    Swal.fire('Error', response.data.message, 'error');\r\n                }\r\n    \r\n            } catch (error) {\r\n                Swal.fire('Error', 'Failed to add brand', 'error');\r\n            }\r\n        };\r\n\r\n        const openEditModal = async (id) => {\r\n            const response = await axios.get(`${apiURL}/get_brand/${id}`);\r\n            const brand = response.data.brand;\r\n            editBrand.value = {\r\n                _id: brand._id,\r\n                name: brand.name,\r\n                logo: { url: `${imageURL}${brand.logo}`, file: null },\r\n                status: brand.status,\r\n            };\r\n        };\r\n\r\n        const updateBrand = async () => {\r\n    try {\r\n        const formData = new FormData();\r\n        formData.append('name', editBrand.value.name); // Ensure `editBrand.value.name` exists\r\n        formData.append('status', editBrand.value.status);\r\n        formData.append('logoRemoved', editBrand.value.isLogoRemoved) // Ensure `editBrand.value.status` exists\r\n        if (editBrand.value.logo.file) {\r\n            formData.append('logo', editBrand.value.logo.file); // Only append if a new file is selected\r\n        }\r\n\r\n        const response = await axios.patch(`${apiURL}/edit_brand/${editBrand.value._id}`, formData);\r\n\r\n        if (response.data.success) {\r\n            Swal.fire('Success', 'Brand updated successfully!', 'success').then(() => {\r\n\r\n                document.querySelector('#edit-brand .btn-cancel').click(); // Close modal\r\n            });\r\n        } else {\r\n            Swal.fire('Error', response.data.message, 'error');\r\n        }\r\n        getBrands(); // Refresh the brand list\r\n    } catch (error) {\r\n        console.error('Error in updateBrand:', error);\r\n        Swal.fire('Error', error.message, 'error');\r\n    }\r\n};\r\n\r\n\r\n        const deleteBrand = async (id) => {\r\n            Swal.fire({\r\n                title: 'Are you sure?',\r\n                text: 'You are about to delete this brand!',\r\n                icon: 'warning',\r\n                showCancelButton: true,\r\n                confirmButtonText: 'Yes, delete it!',\r\n            }).then(async (result) => {\r\n                if (result.isConfirmed) {\r\n                    try {\r\n                        const response = await axios.patch(`${apiURL}/edit_brand/${id}`, {\r\n                            status: 'deleted',\r\n                            deletedAt: Date.now(),\r\n                        });\r\n                        if (response.data.success) {\r\n                            Swal.fire('Success', 'Brand deleted successfully!', 'success');