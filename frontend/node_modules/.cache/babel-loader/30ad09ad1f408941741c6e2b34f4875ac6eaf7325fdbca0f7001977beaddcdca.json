ed, onBeforeUnmount,nextTick } from 'vue';\r\nimport axios from 'axios';\r\nimport Swal from 'sweetalert2';\r\nimport $ from 'jquery';\r\nimport 'select2'; \r\nimport feather from 'feather-icons'\r\nimport ClipLoader from 'vue-spinner/src/ClipLoader.vue';\r\n\r\n\r\nimport Navbar from '/src/components/Admin/Navbar.vue';\r\n\r\n\r\nexport default {\r\n  components: {\r\n    Navbar,\r\n    ClipLoader,\r\n  },\r\n  setup() {\r\n    const select = ref('.select'); \r\n    const brands = ref([]);\r\n    const newBrand = ref({\r\n        name: '',\r\n        logo: '',\r\n        status: 'active'\r\n    });\r\n    const editBrand = ref({\r\n        id: '',\r\n        name: '',\r\n        logo: '',\r\n        status: 'active'\r\n    })\r\n    const loading = ref(true);\r\n\r\n    const getBrands = async () =>{\r\n        loading.value = true;\r\n        try {\r\n            const response = await axios.get('http://localhost:5000/api/brands_list');\r\n            brands.value = response.data.brands;\r\n            if(!response.data.success){\r\n                Swal.fire('Error', response.data.message,'error')\r\n            }         \r\n        } catch (error) {\r\n            console.error(error); \r\n            Swal.fire('Error', 'Failed to get brands','error')\r\n        }finally {\r\n            loading.value = false;\r\n        }\r\n    }\r\n\r\n    onMounted(async () => {\r\n        try{\r\n         $(select.value).select2();\r\n        await feather.replace();\r\n         $('.table').DataTable();\r\n        }catch(error){\r\n            console.log(error);\r\n        }\r\n\r\n    });\r\n    onBeforeUnmount(() => {\r\n      $(select.value).select2('destroy');\r\n    });\r\n    return {\r\n      select,\r\n      brands,\r\n      newBrand,\r\n      editBrand,\r\n      loading,\r\n      getBrands,\r\n    };\r\n  },\r\n};\r\n</script>"],"mappings":"AAiNA,SAASA,GAAG,EAAEC,SAAS,EAAEC,eAAe,EAACC,QAAO,QAAS,KAAK;AAC9D,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAC9B,OAAOC