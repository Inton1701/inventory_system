QuantityAlert.value)\n        });\n        if (response.data.success) {\n          Swal.fire('Success', 'Stock updated successfully.', 'success');\n          await getProductList(); // Refresh product list\n          const modal = bootstrap.Modal.getInstance(document.getElementById('edit-stock'));\n          modal.hide();\n        } else {\n          Swal.fire('Failed', 'Unable to update stock.', 'error');\n        }\n      } catch (error) {\n        console.error('Error updating stock:', error);\n        Swal.fire('Error', 'Failed to update stock.', 'error');\n      }\n    };\n    onMounted(() => {\n      getProductList();\n    });\n    return {\n      products,\n      loading,\n      currentSKU,\n      editQuantity,\n      editQuantityAlert,\n      openEditStockModal,\n      submitStockUpdate\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","Swal","$","setup","products","loading","currentSKU","editQuantity","editQuantityAlert","selectedProductId","getProductList","value","response","get","data","productList","console","log","error","openEditStockModal","productId","quantity","quantityAlert","sku","modal","bootstrap","Modal","document","getElementById","show","alert","submitStockUpdate","put","Number","success","fire","getInstance","hide"],"sources":["C:\\Users\\Administrator\\OneDrive\\Desktop\\POSExpress\\frontend\\src\\components\\Admin\\Inventory\\ManageStocks.vue"],"sourcesContent":["<template>\r\n    <Navbar />\r\n\r\n    <div class=\"page-wrapper\">\r\n        <ClipLoader v-if=\"loading\" />\r\n        <div class=\"content\">\r\n            <div class=\"page-header\">\r\n                <div class=\"page-title me-auto\">\r\n                    <h4>Manage Stocks</h4>\r\n\r\n                </div>\r\n                <ul class=\"table-top-head low-stock-top-head\">\r\n                    <li>\r\n                        <div class=\"status-toggle d-flex justify-content-between align-items-center\">\r\n                            <input typ