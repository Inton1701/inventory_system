;\n        console.log(newBrand.value);\n        if (!response.data.success) {\n          Swal.fire('Error', response.data.message, 'error');\n        } else {\n          Swal.fire('Success', 'Brand successfully added!', 'success');\n        }\n      } catch (error) {\n        console.error(error);\n        Swal.fire('Error', 'Failed to add brand', 'error');\n      } finally {\n        getBrands();\n      }\n    };\n    onMounted(async () => {\n      try {\n        nextTick(async () => {\n          getBrands();\n          await feather.replace();\n        });\n        $('.table').DataTable();\n      } catch (error) {\n        console.log(error);\n      }\n    });\n    onBeforeUnmount(async () => {\n      try {\n        await getBrands();\n        feather.replace();\n      } catch (error) {\n        console.log(error);\n      }\n    });\n    const triggerFileInput = () => {\n      if (fileInput.value) {\n        fileInput.value.click(); // Programmatically click the input\n        feather.replace();\n      }\n    };\n    const onFileChange = (data, event) => {\n      feather.replace();\n      const file = event.target.files[0];\n      if (file) {\n        data.logo = {\n          file,\n          url: URL.createObjectURL(file)\n        }; // Update logo property\n      }\n    };\n    const removeImage = async data => {\n      await feather.replace();\n      data.logo = {\n        url: ''\n      }; // Clear the logo object to reset image\n    };\n    return {\n      brands,\n      newBrand,\n      editBrand,\n      loading,\n      getBrands,\n      onFileChange,\n      removeImage,\n      triggerFileInput,\n      addBrand,\n      fileInput\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onBeforeUnmount","nextTick","axios","Swal","feather","ClipLoader","$","Navbar","components","setup","brands","newBrand","name","logo","status","editBrand","id","loading","fileInput","getBrands","value","response","get","data","success","fire","message","error","console