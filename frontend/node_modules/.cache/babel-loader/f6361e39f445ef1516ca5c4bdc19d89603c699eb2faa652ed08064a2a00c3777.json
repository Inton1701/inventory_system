                }\r\n                const response = await axios.post('http://localhost:5000/api/add_brand', formData, {\r\n                    headers: {\r\n                        'Content-Type': 'multipart/form-data', // Ensure proper headers\r\n                    },\r\n                });\r\n\r\n                console.log(newBrand.value);\r\n                if (!response.data.success) {\r\n                    Swal.fire('Error', response.data.message, 'error');\r\n                } else {\r\n                    Swal.fire('Success', 'Brand successfully added!', 'success');\r\n                }\r\n            } catch (error) {\r\n                console.error(error);\r\n                Swal.fire('Error', 'Failed to add brand', 'error');\r\n            } finally {\r\n                getBrands();\r\n            }\r\n        };\r\n\r\n        onMounted(async () => {\r\n            try {\r\n                nextTick(async () => {\r\n                    getBrands();\r\n                    await feather.replace();\r\n                })\r\n                $('.table').DataTable();\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n\r\n        });\r\n        onBeforeUnmount(async () => {\r\n            try {\r\n                await getBrands();\r\n                feather.replace();\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n\r\n        });\r\n        const triggerFileInput = () => {\r\n            if (fileInput.value) {\r\n                fileInput.value.click(); // Programmatically click the input\r\n                feather.replace();\r\n            }\r\n        };\r\n        const onFileChange = (data, event) => {\r\n            feather.replace()\r\n            const file = event.target.files[0];\r\n\r\n            if (file) {\r\n                data.logo = { file, url: URL.createObjectURL(file) }; // Update logo property\r\n            }\r\n        };\r\n\r\n        const removeI