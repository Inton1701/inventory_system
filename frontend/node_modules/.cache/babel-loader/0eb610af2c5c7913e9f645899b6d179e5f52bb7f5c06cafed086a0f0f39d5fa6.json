       <span aria-hidden=\"true\">&times;</span>\r\n                    </button>\r\n                </div>\r\n                <div class=\"modal-body custom-modal-body\">\r\n                    <form @submit.prevent=\"updateCategory\">\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Category</label>\r\n                            <input type=\"text\" class=\"form-control\" v-model=\"editCategory.name\" required />\r\n                        </div>\r\n                        <div class=\"mb-0\">\r\n                            <div class=\"status-toggle modal-status d-flex justify-content-between align-items-center\">\r\n                                <span class=\"status-label\">Status</span>\r\n                                <input type=\"checkbox\" id=\"edit-category-status\" v-model=\"editCategory.active\" />\r\n                                <label for=\"edit-category-status\" class=\"checktoggle\"></label>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"modal-footer-btn\">\r\n                            <button type=\"button\" class=\"btn btn-cancel me-2\" data-bs-dismiss=\"modal\">Cancel</button>\r\n                            <button type=\"submit\" class=\"btn btn-submit\">Save Changes</button>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport axios from 'axios';\r\nimport Swal from 'sweetalert2';\r\nimport Navbar from '/src/components/Admin/Navbar.vue';\r\nimport ClipLoader from 'vue-spinner/src/ClipLoader.vue';\r\n\r\nexport default {\r\n    components: { \r\n        Navbar,\r\n        ClipLoader\r\n     },\r\n    setup() {\r\n        const categories = ref([]);\r\n        const newCategory = ref({ name: '', active: true });\r\n        const editCategory = ref({ id: null, name: '', active: true });\r\n        const loading = ref(true);\r\n\r\n        const fetchCategories = async () => {\r\n            loading.value = true;\r\n            try {\r\n                const response = await axios.get('http://localhost:5000/api/get_category_list');\r\n                categories.value = response.data.categories;\r\n                if(!response.data.success){\r\n                    Swal.fire('Error', response.data.message, 'errror');\r\n                }\r\n            } catch (error) {\r\n                console.error(error);\r\n                Swal.fire('Error', 'Unable to fetch categories', 'error');\r\n            }\r\n            finally {\r\n                loading.value = false;\r\n            }\r\n        };\r\n\r\n        const addCategory = async () => {\r\n            try {\r\n                await axios.post('http://localhost:5000/api/add_category', newCategory.value);\r\n                Swal.fire('Success', 'Category added successfully!', 'success');\r\n                fetchCategories();\r\n                document.querySelector('#add-category .btn-cancel').click();\r\n            } catch (error) {\r\n                console.error(error);\r\n                Swal.fire('Error', 'Failed to add category', 'error');\r\n            }\r\n        };\r\n\r\n        const openEditModal = (category) => {\r\n            editCategory.value = { ...category };\r\n            const editModal = new bootstrap.Modal(document.getElementById('edit-category'));\r\n            editModal.show();\r\n        };\r\n\r\n        const updateCategory = async () => {\r\n            try {\r\n                await axios.put(`/api/categories/${editCategory.value.id}`, editCategory.value);\r\n                Swal.fire('Success', 'Category updated successfully!', 'success');\r\n                fetchCategories();\r\n                document.querySelector('#edit-category .btn-cancel').click();\r\n            } catch (error) {\r\n                console.error(error);\r\n                Swal.fire('Error', 'Failed to update category', 'error');\r\n            }\r\n        };\r\n\r\n        const deleteCategory = async (id) => {\r\n            Swal.fire({\r\n                title: 'Are you sure?',\r\n                text: 'You won\\'t be able to revert this!',\r\n                icon: 'warning',\r\n                showCancelButton: true,\r\n                confirmButtonText: 'Yes, delete it!',\r\n            }).then(async (result) => {\r\n                if (result.isConfirmed) {\r\n                    try {\r\n                        await axios.delete(`/api/categories/${id}`);\r\n                        Swal.fire('Deleted!', 'Category has been deleted.', 'success');\r\n                        fetchCategories();\r\n                    } catch (error) {\r\n                        console.error(error);\r\n                        Swal.fire('Error', 'Failed to delete category', 'error');\r\n                    }\r\n                }\r\n            });\r\n        };\r\n\r\n        onMounted(fetchCategories);\r\n\r\n        return {\r\n            categories,\r\n            newCategory,\r\n            editCategory,\r\n            fetchCategories,\r\n            addCategory,\r\n            openEditModal,\r\n            updateCategory,\r\n            deleteCategory,\r\n        };\r\n    },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Add your custom styles here */\r\n</style>\r\n"],"mappings":"AA4IA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAC9B,OAAOC,MAAK,MAAO,kCAAkC;AACrD,OAAOC,UAAS,MAAO,gCAAgC;AAEvD,eAAe;EACXC,UAAU,EAAE;IACRF,MAAM;IACNC;EACH,CAAC;EACFE,KAAKA,CAAA,EAAG;IACJ,MAAMC,UAAS,GAAIR,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMS,WAAU,GAAIT,GAAG,CAAC;MAAEU,IAAI,EAAE,EAAE;MAAEC,MAAM,EAAE;IAAK,CAAC,CAAC;IACnD,MAAMC,YAAW,GAAIZ,GAAG,CAAC;MAAEa,EAAE,EAAE,IAAI;MAAEH,IAAI,EAAE,EAAE;MAAEC,MAAM,EAAE;IAAK,CAAC,CAAC;IAC9D,MAAMG,OAAM,GAAId,GAAG,CAAC,IAAI,CAAC;IAEzB,MAAMe,eAAc,GAAI,MAAAA,CAAA,KAAY;MAChC