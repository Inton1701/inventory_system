{"ast":null,"code":"import { ref, onMounted, onBeforeMount } from 'vue';\nimport axios from 'axios';\nimport Swal from 'sweetalert2';\nimport Navbar from '/src/components/Admin/Navbar.vue';\nimport ClipLoader from 'vue-spinner/src/ClipLoader.vue';\nimport feather from 'feather-icons';\nimport $ from 'jquery';\nexport default {\n  components: {\n    Navbar,\n    ClipLoader\n  },\n  setup() {\n    const categories = ref([]);\n    const newCategory = ref({\n      name: '',\n      status: 'inactive'\n    });\n    const editCategory = ref({\n      id: null,\n      name: '',\n      active: null\n    });\n    const loading = ref(true);\n    const getCategories = async () => {\n      loading.value = true;\n      try {\n        const response = await axios.get('http://localhost:5000/api/get_category_list');\n        categories.value = response.data.categories;\n        if (!response.data.success) {\n          Swal.fire('Error', response.data.message, 'errror');\n        }\n      } catch (error) {\n        console.error(error);\n        Swal.fire('Error', 'Unable to fetch categories', 'error');\n      } finally {\n        loading.value = false;\n      }\n    };\n    const toggleStatus = (data, event) => {\n      data.status = event.target.checked ? 'active' : 'inactive';\n    };\n    const addCategory = async () => {\n      try {\n        const response = await axios.post('http://localhost:5000/api/add_category', newCategory.value);\n        if (response.data.success) {\n          Swal.fire('Success', 'Category added successfully!', 'success');\n          document.querySelector('#add-category .btn-cancel').click();\n          await getCategories();\n        } else {\n          Swal.fire('Error', response.data.message || 'Failed to add category', 'error');\n        }\n      } catch (error) {\n        console.error(error);\n        const errorMessage = error.response?.data?.message || 'An unexpected error occurred';\n        Swal.fire('Error', errorMessage, 'error');\n      }\n    };\n    const openEditModal = category => {\n      editCategory.value = {\n        ...category\n      };\n      const editModal = new bootstrap.Modal(document.getElementById('edit-category'));\n      editModal.show();\n    };\n    const updateCategory = async () => {\n      try {\n        await axios.put(`/api/categories/${editCategory.value.id}`, editCategory.value);\n        Swal.fire('Success', 'Category updated successfully!', 'success');\n        document.querySelector('#edit-category .btn-cancel').click();\n      } catch (error) {\n        console.error(error);\n        Swal.fire('Error', 'Failed to update category', 'error');\n      }\n    };\n    const deleteCategory = async id => {\n      Swal.fire({\n        title: 'Are you sure?',\n        text: 'You won\\'t be able to revert this!',\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonText: 'Yes, delete it!'\n      }).then(async result => {\n        if (result.isConfirmed) {\n          try {\n            await axios.delete(`/api/categories/${id}`);\n            Swal.fire('Deleted!', 'Category has been deleted.', 'success');\n            fetchCategories();\n          } catch (error) {\n            console.error(error);\n            Swal.fire('Error', 'Failed to delete category', 'error');\n          }\n        }\n      });\n    };\n    onMounted(async () => {\n      try {\n        await getCategories();\n        await $(select.value).select2();\n        feather.replace();\n        await $('.table').DataTable();\n      } catch (error) {\n        console.log(error);\n      }\n    });\n    onBeforeMount();\n    foreMount();\n    return {\n      categories,\n      newCategory,\n      editCategory,\n      getCategories,\n      addCategory,\n      openEditModal,\n      updateCategory,\n      deleteCategory,\n      toggleStatus,\n      loading\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onBeforeMount","axios","Swal","Navbar","ClipLoader","feather","$","components","setup","categories","newCategory","name","status","editCategory","id","active","loading","getCategories","value","response","get","data","success","fire","message","error","console","toggleStatus","event","target","checked","addCategory","post","document","querySelector","click","errorMessage","openEditModal","category","editModal","bootstrap","Modal","getElementById","show","updateCategory","put","deleteCategory","title","text","icon","showCancelButton","confirmButtonText","then","result","isConfirmed","delete","fetchCategories","select","select2","replace","DataTable","log","foreMount"],"sources":["C:\\Users\\Administrator\\OneDrive\\Desktop\\POSExpress\\frontend\\src\\components\\Admin\\Inventory\\Category.vue"],"sourcesContent":["<template>\r\n    <Navbar />\r\n    <div class=\"page-wrapper\">\r\n        <ClipLoader v-if=\"loading\" />\r\n        <div class=\"content\">\r\n            <div class=\"page-header\">\r\n                <div class=\"add-item d-flex\">\r\n                    <div class=\"page-title\">\r\n                        <h4>Category</h4>\r\n                        <h6>Manage your categories</h6>\r\n                    </div>\r\n                </div>\r\n                <div class=\"page-btn\">\r\n                    <a href=\"#\" class=\"btn btn-added\" data-bs-toggle=\"modal\" data-bs-target=\"#add-category\">\r\n                        <i data-feather=\"plus-circle\" class=\"me-2\"></i>Add New Category</a>\r\n                </div>\r\n            </div>\r\n            <div class=\"card table-list-card\">\r\n                <div class=\"card-body\">\r\n                    <div class=\"table-responsive\">\r\n\r\n                        <table class=\"table datanew\">\r\n                            <thead>\r\n                                <tr>\r\n                                    <th>Category</th>\r\n                                    <th>Created On</th>\r\n                                    <th>Status</th>\r\n                                    <th class=\"no-sort\">Action</th>\r\n                                </tr>\r\n                            </thead>\r\n                            <tbody>\r\n\r\n                                <tr v-if=\"categories.length <= 0\">\r\n                                    <td colspan=\"8\">No products available</td>\r\n                                </tr>\r\n                                <tr v-else v-for=\"category in categories\" :key=\"category.id\">\r\n                                    <td>{{ category.name }}</td>\r\n                                    <td>{{ category.createdAt }}</td>\r\n                                    <td>\r\n\r\n                                        <span :class=\"{\r\n                                            'badge': true,\r\n                                            'badge-linesuccess': category.status === 'active',\r\n                                            'badge-linewarning': category.status === 'inactive'\r\n                                        }\">\r\n                                            {{ category.status }}\r\n                                        </span>\r\n                                    </td>\r\n\r\n                                    <td class=\"action-table-data\">\r\n                                        <div class=\"edit-delete-action\">\r\n                                            <a class=\"me-2 p-2\" href=\"#\" data-bs-toggle=\"modal\"\r\n                                                @click=\"openEditModal(category)\">\r\n                                                <i data-feather=\"edit\" class=\"feather-edit\"></i>\r\n                                            </a>\r\n                                            <a class=\"confirm-text p-2\" href=\"javascript:void(0);\"\r\n                                                @click=\"deleteCategory(category.id)\">\r\n                                                <i data-feather=\"trash-2\" class=\"feather-trash-2\"></i>\r\n                                            </a>\r\n                                        </div>\r\n                                    </td>\r\n                                </tr>\r\n                            </tbody>\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- Add Category Modal -->\r\n    <div class=\"modal fade\" id=\"add-category\">\r\n        <div class=\"modal-dialog modal-dialog-centered custom-modal-two\">\r\n            <div class=\"modal-content\">\r\n                <div class=\"modal-header border-0 custom-modal-header\">\r\n                    <div class=\"page-title\">\r\n                        <h4>Create Category</h4>\r\n                    </div>\r\n                    <button type=\"button\" class=\"close\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                        <span aria-hidden=\"true\">&times;</span>\r\n                    </button>\r\n                </div>\r\n                <div class=\"modal-body custom-modal-body\">\r\n                    <form @submit.prevent=\"addCategory\">\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Category</label>\r\n                            <input type=\"text\" class=\"form-control\" v-model=\"newCategory.name\" required />\r\n                        </div>\r\n                        <div class=\"mb-0\">\r\n                            <div class=\"status-toggle modal-status d-flex justify-content-between\">\r\n                                <span class=\"form-label\">{{ newCategory.status }}</span>\r\n                                <input type=\"checkbox\" id=\"user4\" class=\"check\"\r\n                                    :checked=\"newCategory.status === 'active'\" @change=\"toggleStatus(newCategory, $event)\" />\r\n                                <label for=\"user4\" class=\"checktoggle\"></label>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"modal-footer-btn\">\r\n                            <button type=\"button\" class=\"btn btn-cancel me-2\" data-bs-dismiss=\"modal\">Cancel</button>\r\n                            <button type=\"submit\" class=\"btn btn-submit\">Create Category</button>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- Edit Category Modal -->\r\n    <div class=\"modal fade\" id=\"edit-category\">\r\n        <div class=\"modal-dialog modal-dialog-centered custom-modal-two\">\r\n            <div class=\"modal-content\">\r\n                <div class=\"modal-header border-0 custom-modal-header\">\r\n                    <div class=\"page-title\">\r\n                        <h4>Edit Category</h4>\r\n                    </div>\r\n                    <button type=\"button\" class=\"close\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\r\n                        <span aria-hidden=\"true\">&times;</span>\r\n                    </button>\r\n                </div>\r\n                <div class=\"modal-body custom-modal-body\">\r\n                    <form @submit.prevent=\"updateCategory\">\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Category</label>\r\n                            <input type=\"text\" class=\"form-control\" v-model=\"editCategory.name\" required />\r\n                        </div>\r\n                        <div class=\"mb-0\">\r\n                            <div class=\"status-toggle modal-status d-flex justify-content-between align-items-center\">\r\n                                <span class=\"status-label\">Status</span>\r\n                                <input type=\"checkbox\" id=\"edit-category-status\" checked\r\n                                    v-model=\"editCategory.active\" />\r\n                                <label for=\"edit-category-status\" class=\"checktoggle\"></label>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"modal-footer-btn\">\r\n                            <button type=\"button\" class=\"btn btn-cancel me-2\" data-bs-dismiss=\"modal\">Cancel</button>\r\n                            <button type=\"submit\" class=\"btn btn-submit\">Save Changes</button>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted,onBeforeMount } from 'vue';\r\nimport axios from 'axios';\r\nimport Swal from 'sweetalert2';\r\nimport Navbar from '/src/components/Admin/Navbar.vue';\r\nimport ClipLoader from 'vue-spinner/src/ClipLoader.vue';\r\nimport feather from 'feather-icons'\r\nimport $ from 'jquery';\r\n\r\nexport default {\r\n    components: {\r\n        Navbar,\r\n        ClipLoader\r\n    },\r\n\r\n    setup() {\r\n        const categories = ref([]);\r\n        const newCategory = ref({ name: '', status: 'inactive' });\r\n        const editCategory = ref({ id: null, name: '', active: null });\r\n        const loading = ref(true);\r\n\r\n        const getCategories = async () => {\r\n            loading.value = true;\r\n            try {\r\n                const response = await axios.get('http://localhost:5000/api/get_category_list');\r\n                categories.value = response.data.categories;\r\n                if (!response.data.success) {\r\n                    Swal.fire('Error', response.data.message, 'errror');\r\n                }\r\n            } catch (error) {\r\n                console.error(error);\r\n                Swal.fire('Error', 'Unable to fetch categories', 'error');\r\n            }\r\n            finally {\r\n                loading.value = false;\r\n            }\r\n        };\r\n        const toggleStatus = (data, event) => {\r\n            data.status = event.target.checked ? 'active' : 'inactive';\r\n        };\r\n\r\n\r\n        const addCategory = async () => {\r\n    try {\r\n        const response = await axios.post('http://localhost:5000/api/add_category', newCategory.value);\r\n\r\n        if (response.data.success) {\r\n            Swal.fire('Success', 'Category added successfully!', 'success');\r\n            document.querySelector('#add-category .btn-cancel').click();\r\n            await getCategories();\r\n        } else {\r\n            Swal.fire('Error', response.data.message || 'Failed to add category', 'error');\r\n        }\r\n    } catch (error) {\r\n        console.error(error);\r\n        const errorMessage = error.response?.data?.message || 'An unexpected error occurred';\r\n        Swal.fire('Error', errorMessage, 'error');\r\n    }\r\n};\r\n\r\n        const openEditModal = (category) => {\r\n            editCategory.value = { ...category };\r\n            const editModal = new bootstrap.Modal(document.getElementById('edit-category'));\r\n            editModal.show();\r\n        };\r\n\r\n        const updateCategory = async () => {\r\n            try {\r\n                await axios.put(`/api/categories/${editCategory.value.id}`, editCategory.value);\r\n                Swal.fire('Success', 'Category updated successfully!', 'success');\r\n                document.querySelector('#edit-category .btn-cancel').click();\r\n            } catch (error) {\r\n                console.error(error);\r\n                Swal.fire('Error', 'Failed to update category', 'error');\r\n            }\r\n        };\r\n\r\n        const deleteCategory = async (id) => {\r\n            Swal.fire({\r\n                title: 'Are you sure?',\r\n                text: 'You won\\'t be able to revert this!',\r\n                icon: 'warning',\r\n                showCancelButton: true,\r\n                confirmButtonText: 'Yes, delete it!',\r\n            }).then(async (result) => {\r\n                if (result.isConfirmed) {\r\n                    try {\r\n                        await axios.delete(`/api/categories/${id}`);\r\n                        Swal.fire('Deleted!', 'Category has been deleted.', 'success');\r\n                        fetchCategories();\r\n                    } catch (error) {\r\n                        console.error(error);\r\n                        Swal.fire('Error', 'Failed to delete category', 'error');\r\n                    }\r\n                }\r\n            });\r\n        };\r\n\r\n        onMounted(async () => {\r\n            try {\r\n                await getCategories();\r\n                await $(select.value).select2();\r\n                feather.replace();\r\n                await $('.table').DataTable();\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n        });\r\n        onBeforeMount();foreMount();\r\n\r\n        return {\r\n            categories,\r\n            newCategory,\r\n            editCategory,\r\n            getCategories,\r\n            addCategory,\r\n            openEditModal,\r\n            updateCategory,\r\n            deleteCategory,\r\n            toggleStatus,\r\n            loading,\r\n\r\n        };\r\n    },\r\n};\r\n</script>\r\n\r\n\r\n"],"mappings":"AAgJA,SAASA,GAAG,EAAEC,SAAS,EAACC,aAAY,QAAS,KAAK;AAClD,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAC9B,OAAOC,MAAK,MAAO,kCAAkC;AACrD,OAAOC,UAAS,MAAO,gCAAgC;AACvD,OAAOC,OAAM,MAAO,eAAc;AAClC,OAAOC,CAAA,MAAO,QAAQ;AAEtB,eAAe;EACXC,UAAU,EAAE;IACRJ,MAAM;IACNC;EACJ,CAAC;EAEDI,KAAKA,CAAA,EAAG;IACJ,MAAMC,UAAS,GAAIX,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMY,WAAU,GAAIZ,GAAG,CAAC;MAAEa,IAAI,EAAE,EAAE;MAAEC,MAAM,EAAE;IAAW,CAAC,CAAC;IACzD,MAAMC,YAAW,GAAIf,GAAG,CAAC;MAAEgB,EAAE,EAAE,IAAI;MAAEH,IAAI,EAAE,EAAE;MAAEI,MAAM,EAAE;IAAK,CAAC,CAAC;IAC9D,MAAMC,OAAM,GAAIlB,GAAG,CAAC,IAAI,CAAC;IAEzB,MAAMmB,aAAY,GAAI,MAAAA,CAAA,KAAY;MAC9BD,OAAO,CAACE,KAAI,GAAI,IAAI;MACpB,IAAI;QACA,MAAMC,QAAO,GAAI,MAAMlB,KAAK,CAACmB,