tus: 'deleted', // Update the status to 'deleted'\r\n          });\r\n\r\n          if (response.data.success) {\r\n            Swal.fire('Deleted!', 'Your product has been deleted.', 'success');\r\n            this.products = this.products.filter(product => product._id !== productId);\r\n          } else {\r\n            Swal.fire('Failed', 'There was an issue deleting the product.', 'error');\r\n          }\r\n        } catch (error) {\r\n          console.error('Error updating product:', error);\r\n          Swal.fire('Error', 'Failed to delete the product.', 'error');\r\n        }\r\n      } else {\r\n        Swal.fire('Cancelled', 'Your product is safe :)', 'error');\r\n      }\r\n    },\r\n  },\r\n\r\n\r\n};\r\n</script>"],"mappings":";;AAyKA,SAASA,GAAG,EAAEC,SAAS,EAAEC,eAAc,QAAS,KAAK;AACrD,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAO,SAAS;AAChB,OAAOC,OAAM,MAAO,eAAe;AACnC,OAAOC,UAAS,MAAO,gCAAgC;AACvD,OAAOC,IAAG,MAAO,aAAa;AAC9B,OAAOC,MAAK,MAAO,kCAAkC;AACrD,OAAOC,CAAA,MAAO,QAAQ;AAEtB,eAAe;EACbC,UAAU,EAAE;IACVF,MAAM;IACNF;EACF,CAAC;EACDK,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIX,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMY,OAAM,GAAIZ,GAAG,CAAC,IAAI,CAAC;IACzB,MAAMa,MAAK,GAAIb,GAAG,CAAC,IAAI,CAAC;IACxB,MAAMc,KAAI,GAAId,GAAG,CAAC,IAAI,CAAC;;IAEvB;IACA,MAAMe,cAAa,GAAI,MAAAA,CAAA,KAAY;MACjCH,OAAO,CAACI,KAAI,GAAI,IAAI;MACpB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,yCAAyC,CAAC;QAC3EP,QAAQ,CAACK,KAAI,GAAIC,QAAQ,CAACE,IAAI,CAACC,WAAW;MAE5C,EAAE,OAAOC,KAAK,EAAE;QACdf,IAAI,CAACgB,IAAI,CAAC,OAAO,EAAC,wBAAwB,EAAC,OAAO;MACpD,UAAU;QACRV,OAAO,CAACI,KAAI,GAAI,KAAK;MACvB;IACF,CAAC;IACDf,SAAS,CAAC,YAAY;MACpB,IAAI;QACF,MAAMc,cAAc,CAAC,CAAC;QACtB,MAAMP,CAAC,CAACK,MAAM,CAACG,KAAK,CAAC,CAACO,OAAO,CAAC,CAAC;QAC9BnB,OAAO,CAACoB,OAAO,CAAC,CAAC;QAClB,MAAMhB,CAAC,CAAC,QAAQ,CAAC,CAACiB,SAAS,CAAC,CAAC;QAC7B,MAAMC,QAAQ,CAAC;QACP,MAAMC,SAAS,CAAC,CAAC;QACjBD,QAAQ,CAAC,MAAM;UACX,MAAMZ,KAAI,GAAIc,QAAQ,CAACC,aAAa,CAAC,QAAQ,CAAC;UAC9C,IAAIf,KAAK,EAAE;YACPN,CAAC,CAACM,KAAK,CAAC,CAACW,SAAS,CAAC,CAAC;UACxB;QACJ,CAAC;MA