 newCategory.value = { name: \"\", status: \"inactive\" };\r\n          await getCategories();\r\n  \r\n          addModal.hide(); // Close modal programmatically\r\n        } else {\r\n          Swal.fire(\"Error\", response.data.message || \"Failed to add category\", \"error\");\r\n        }\r\n      } catch (error) {\r\n        console.error(error);\r\n        Swal.fire(\"Error\", error.response?.data?.message || \"An unexpected error occurred\", \"error\");\r\n      }\r\n    };\r\n\r\n    const openEditModal = async (id) => {\r\n      try {\r\n        const response = await axios.get(\r\n          `http://localhost:5000/api/get_category/${id}`\r\n        );\r\n        editCategory.value = response.data.category;\r\n        const editModal = new bootstrap.Modal(document.getElementById(\"edit-category\"));\r\n        editModal.show(); // Open modal programmatically\r\n      } catch (error) {\r\n        console.error(error);\r\n        Swal.fire(\"Error\", \"Unable to fetch category details\", \"error\");\r\n      }\r\n    };\r\n\r\n    const updateCategory = async () => {\r\n      try {\r\n        const response = await axios.put(\r\n          `http://localhost:5000/api/edit_category/${editCategory.value._id}`,\r\n          editCategory.value\r\n        );\r\n        if (response.data.success) {\r\n          Swal.fire(\"Success\", \"Category updated successfully!\", \"success\");\r\n          await getCategories();\r\n          const editModal = new bootstrap.Modal(document.getElementById(\"edit-category\"));\r\n          editModal.hide(); // Close modal programmatically\r\n        } else {\r\n          Swal.fire(\"Error\", response.data.message, \"error\");\r\n        }\r\n      } catch (error) {\r\n        console.error(error);\r\n        Swal.fire(\"Error\", \"Failed to update category\", \"error\");\r\n      }\r\n    };\r\n\r\n    const deleteCategory = async (id) => {\r\n      Swal.fire({\r\n        title: \"Are you sure?\",\r\n        text: \"You won't be ab