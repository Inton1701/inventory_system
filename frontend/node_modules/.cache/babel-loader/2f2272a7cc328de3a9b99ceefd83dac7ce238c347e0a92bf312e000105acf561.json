{"ast":null,"code":"import \"core-js/modules/esnext.iterator.map.js\";\nimport { ref, onMounted, onBeforeUnmount } from \"vue\";\nimport Chart from \"chart.js/auto\"; // Import Chart.js (automatically includes necessary components like line, bar, etc.)\nimport axios from \"axios\";\nimport Navbar from \"/src/components/Admin/Navbar.vue\";\nexport default {\n  components: {\n    Navbar\n  },\n  data() {\n    return {\n      widgets: [{\n        title: \"Total Items Sold\",\n        value: \"Loading...\",\n        imgSrc: \"assets/img/icons/dash1.svg\",\n        class: \"\"\n      }, {\n        title: \"Total Sales\",\n        value: \"Loading...\",\n        imgSrc: \"assets/img/icons/dash2.svg\",\n        class: \"dash1\"\n      }, {\n        title: \"Total Profit\",\n        value: \"Loading...\",\n        icon: \"fa-arrow-trend-up\",\n        iconColor: \"#74C0FC\",\n        class: \"dash2\"\n      }, {\n        title: \"Total Lost\",\n        value: \"Loading...\",\n        icon: \"fa-arrow-trend-down\",\n        iconColor: \"#ee3a5e\",\n        class: \"dash3\"\n      }, {\n        title: \"Transactions\",\n        value: \"Loading...\",\n        icon: \"fa-handshake\",\n        class: \"\"\n      }, {\n        title: \"Returns\",\n        value: \"Loading...\",\n        icon: \"fa-reply\",\n        class: \"badge-bgprimary\"\n      }, {\n        title: \"Purchase Invoice\",\n        value: \"Loading...\",\n        icon: \"fa-ban\",\n        class: \"bg-danger\"\n      }, {\n        title: \"Completed Transactions\",\n        value: \"Loading...\",\n        icon: \"fa-clipboard-check\",\n        class: \"das3\"\n      }]\n    };\n  },\n  setup() {\n    const chartCanvas = ref(null); // Reference to the canvas element\n    let chartInstance = null; // Variable to hold the Chart.js instance\n    const salesData = ref([]);\n\n    // Fetch the sales data\n    onMounted(async () => {\n      try {\n        const response = await axios.get(\"http://localhost:5000/api/get_sales\");\n        if (response.data.success) {\n          salesData.value = response.data.salesDatas;\n          console.log(\"Sales data fetched:\", salesData.value);\n          renderChart(); // Render the chart once the data is fetched\n        } else {\n          console.error(\"Failed to fetch sales data\");\n        }\n      } catch (error) {\n        console.error(\"Error fetching sales data:\", error);\n      }\n    });\n\n    // Function to render the chart after data is fetched\n    const renderChart = () => {\n      if (salesData.value.length > 0 && chartCanvas.value) {\n        const labels = salesData.value.map(data => data._id); // Assuming '_id' as labels\n        const totalSales = salesData.value.map(data => data.totalSales);\n        const totalProfit = salesData.value.map(data => data.totalProfit);\n        const ctx = chartCanvas.value.getContext(\"2d\"); // Get the context for the canvas element\n\n        // Initialize the chart with the provided data\n        chartInstance = new Chart(ctx, {\n          type: \"bar\",\n          // Bar chart type (vertical by default)\n          data: {\n            labels: labels,\n            // X-axis labels (dates)\n            datasets: [{\n              label: \"Total Sales\",\n              data: totalSales,\n              backgroundColor: \"rgb(40, 199, 111)\",\n              // Bar color for total sales\n              borderColor: \"rgb(75, 192, 192)\",\n              // Bar border color\n              borderWidth: 1\n            }, {\n              label: \"Total Profit\",\n              data: totalProfit,\n              backgroundColor: \"rgb(255, 77, 77)\",\n              // Bar color for total profit\n              borderColor: \"rgb(255, 99, 132)\",\n              // Bar border color\n              borderWidth: 1\n            }]\n          },\n          options: {\n            responsive: true,\n            plugins: {\n              legend: {\n                position: \"top\"\n              },\n              tooltip: {\n                enabled: true\n              }\n            },\n            scales: {\n              x: {\n                title: {\n                  display: true,\n                  text: \"Dates\"\n                }\n              },\n              y: {\n                title: {\n                  display: true,\n                  text: \"Values\"\n                },\n                beginAtZero: true\n              }\n            }\n          }\n        });\n      }\n    };\n    const fetchDashboardData = async () => {\n      try {\n        // Fetch data from your APIs\n        const salesResponse = await axios.get(\"/api/dashboard/sales-data\");\n        const statsResponse = await axios.get(\"/api/dashboard/statistics\");\n\n        // Update the widget data\n        this.widgets[0].value = salesResponse.data.totalItemsSold;\n        this.widgets[1].value = salesResponse.data.totalSales;\n        this.widgets[2].value = salesResponse.data.totalProfit;\n        this.widgets[3].value = salesResponse.data.totalLost;\n        this.widgets[4].value = statsResponse.data.transactions;\n        this.widgets[5].value = statsResponse.data.returns;\n        this.widgets[6].value = statsResponse.data.purchaseInvoices;\n        this.widgets[7].value = statsResponse.data.completedTransactions;\n      } catch (error) {\n        console.error(\"Error fetching dashboard data:\", error);\n      }\n    };\n    onMounted(() => {\n      fetchDashboardData();\n    });\n    // Clean up the chart instance when the component is unmounted\n    onBeforeUnmount(() => {\n      if (chartInstance) {\n        chartInstance.destroy();\n      }\n    });\n    return {\n      chartCanvas,\n      fetchDashboardData // Reference to the canvas element\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onBeforeUnmount","Chart","axios","Navbar","components","data","widgets","title","value","imgSrc","class","icon","iconColor","setup","chartCanvas","chartInstance","salesData","response","get","success","salesDatas","console","log","renderChart","error","length","labels","map","_id","totalSales","totalProfit","ctx","getContext","type","datasets","label","backgroundColor","borderColor","borderWidth","options","responsive","plugins","legend","position","tooltip","enabled","scales","x","display","text","y","beginAtZero","fetchDashboardData","salesResponse","statsResponse","totalItemsSold","totalLost","transactions","returns","purchaseInvoices","completedTransactions","destroy"],"sources":["C:\\Users\\Administrator\\OneDrive\\Desktop\\POSExpress\\frontend\\src\\components\\Admin\\Main\\Dashboard.vue"],"sourcesContent":["<template>\r\n  <Navbar />\r\n\r\n  <div class=\"page-wrapper\">\r\n    <div class=\"content\">\r\n      <div class=\"row\">\r\n    <div class=\"col-xl-3 col-sm-6 col-12 d-flex\" v-for=\"(widget, index) in widgets\" :key=\"index\">\r\n      <div :class=\"['dash-widget', widget.class, 'w-100']\">\r\n        <div class=\"dash-widgetimg\">\r\n          <span>\r\n            <img v-if=\"widget.imgSrc\" :src=\"widget.imgSrc\" alt=\"img\" />\r\n            <font-awesome-icon\r\n              v-if=\"widget.icon\"\r\n              :icon=\"widget.icon\"\r\n              :size=\"widget.iconSize || '1x'\"\r\n              :style=\"{ color: widget.iconColor || 'inherit' }\"\r\n            />\r\n          </span>\r\n        </div>\r\n        <div class=\"dash-widgetcontent\">\r\n          <h5>\r\n            <font-awesome-icon icon=\"fa-peso-sign\" />\r\n            <span>{{ widget.value }}</span>\r\n          </h5>\r\n          <h6>{{ widget.title }}</h6>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n      <div class=\"row\">\r\n        <div class=\"col-xl-7 col-sm-12 col-12 d-flex\">\r\n          <div class=\"card flex-fill\">\r\n            <div class=\"card-header d-flex justify-content-between align-items-center\">\r\n              <h5 class=\"card-title mb-0\">Purchase & Sales</h5>\r\n           