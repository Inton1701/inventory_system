{"ast":null,"code":"import \"core-js/modules/esnext.iterator.map.js\";\nimport { ref, onMounted, onBeforeUnmount } from \"vue\";\nimport Chart from \"chart.js/auto\";\nimport axios from \"axios\";\nimport Navbar from \"/src/components/Admin/Navbar.vue\";\nexport default {\n  components: {\n    Navbar\n  },\n  setup() {\n    const chartCanvas = ref(null); // Reference to the canvas element\n    let chartInstance = null; // Variable to hold the Chart.js instance\n    const salesData = ref([]);\n    const widgets = ref([{\n      title: \"Total Items Sold\",\n      value: \"Loading...\",\n      imgSrc: \"assets/img/icons/dash1.svg\",\n      class: \"\"\n    }, {\n      title: \"Total Sales\",\n      value: \"Loading...\",\n      imgSrc: \"assets/img/icons/dash2.svg\",\n      class: \"dash1\"\n    }, {\n      title: \"Total Profit\",\n      value: \"Loading...\",\n      icon: \"fa-arrow-trend-up\",\n      iconColor: \"#74C0FC\",\n      class: \"dash2\"\n    }, {\n      title: \"Total Lost\",\n      value: \"Loading...\",\n      icon: \"fa-arrow-trend-down\",\n      iconColor: \"#ee3a5e\",\n      class: \"dash3\"\n    }, {\n      title: \"Transactions\",\n      value: \"Loading...\",\n      icon: \"fa-handshake\",\n      class: \"\"\n    }, {\n      title: \"Returns\",\n      value: \"Loading...\",\n      icon: \"fa-reply\",\n      class: \"badge-bgprimary\"\n    }, {\n      title: \"Purchase Invoice\",\n      value: \"Loading...\",\n      icon: \"fa-ban\",\n      class: \"bg-danger\"\n    }, {\n      title: \"Completed Transactions\",\n      value: \"Loading...\",\n      icon: \"fa-clipboard-check\",\n      class: \"das3\"\n    }]);\n\n    // Fetch sales data for the chart\n    const fetchSalesData = async () => {\n      try {\n        const response = await axios.get(\"http://localhost:5000/api/get_sales\");\n        if (response.data.success) {\n          salesData.value = response.data.salesDatas;\n          renderChart(); // Render the chart once the data is fetched\