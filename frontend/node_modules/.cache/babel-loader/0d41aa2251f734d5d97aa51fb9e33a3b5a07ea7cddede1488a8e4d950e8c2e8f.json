c () => {\r\n      const productId = route.params._id; // Get the ID from the route params\r\n      try {\r\n        const response = await axios.get(`http://localhost:5000/api/get_product/${productId}`);\r\n        product.value = response.data.product; // Adjust based on your API response structure\r\n      } catch (error) {\r\n        console.error('Error fetching product details:', error);\r\n      }\r\n    };\r\n\r\n    onMounted(async () => {\r\n      try {\r\n        await $(select.value).select2();\r\n        await feather.replace();\r\n        await $('.table').DataTable();\r\n\r\n        await getProductDetails();\r\n      } catch (error) {\r\n        console.error(error);\r\n      }\r\n    });\r\n\r\n    onBeforeUnmount(() => {\r\n      $(select.value).select2('destroy');\r\n    });\r\n\r\n    return { \r\n      product,\r\n      select\r\n    };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAiGA,SAASA,GAAG,EAAEC,SAAS,EAAEC,eAAc,QAAS,KAAK;AACrD,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,YAAY;AACrC,OAAO,SAAS;AAChB,OAAOC,OAAM,MAAO,eAAe;AACnC,OAAO,oBAAoB;AAC3B,OAAO,sDAAsD;AAE7D,OAAOC,OAAM,MAAO,mCAAmC;AACvD,OAAOC,MAAK,MAAO,kCAAkC;AAErD,eAAe;EACbC,UAAU,EAAE;IACVF,OAAO;IACPC;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIV,GAAG,CAAC,SAAS,CAAC;IAC7B,MAAMW,KAAI,GAAIP,QAAQ,CAAC,CAAC;IACxB,MAAMQ,OAAM,GAAIZ,GAAG,CAAC,IAAI,CAAC,EAAE;;IAE3B,MAAMa,iBAAgB,GAAI,MAAAA,CAAA,KAAY;MACpC,MAAMC,SAAQ,GAAIH,KAAK,CAACI,MAAM,CAACC,GAAG,EAAE;MACpC,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,yCAAyCJ,SAAS,EAAE,CAAC;QACtFF,OAAO,CAACO,KAAI,GAAIF,QAAQ,CAACG,IAAI,CAACR,OAAO,EAAE;MACzC,EAAE,OAAOS,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD;IACF,CAAC;IAEDpB,SAAS,CAAC,YAAY;MACpB,IAAI;QACF,MAAMsB,CAAC,CAACb,MAAM,CAACS,KAAK,CAAC,CAACK,OAAO,CAAC,CAAC;QAC/B,MAAMnB,OAAO,CAACoB,OAAO,CAAC,CAAC;QACvB,MAAMF,CAAC,CAAC,QAAQ,CAAC,CAACG,SAAS,CAAC,CAAC;QAE7B,MAAMb,iBAAiB,CAAC,CAAC;MAC3B,EAAE,OAAOQ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB;IACF,CAAC,CAAC;IAE